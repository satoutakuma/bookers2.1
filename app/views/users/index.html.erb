
  <%= render 'books/list', user: current_user, book: @book %>
  <div class="col-md-8 offset-md-1">
	  <h2>Users users index</h2>
		<table class="table table-hoble-hover table-inverse">
			 <thead>
				 <tr>
				  <th class="header1">image</th>
					 <th class="header2">name</th>
					 <th class="header3"></th>
				 </tr>
			 </thead>
		 	<tbody>
				  <% @users.each do |user| %>
				  <tr>
				   <td>
				    <%= image_tag user.get_profile_image(100,100) %>
				   </td>
				   <td><%= user.name %></td>
				   <td><%= link_to "Show", user_path(user.id) %></td>
				  </tr>
				  <% end %>
			 </tbody>
		 </table>
	 </div>
</div>